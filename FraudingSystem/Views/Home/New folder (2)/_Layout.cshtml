<script>
    function Show_Message(type, message) {
        $("#lblAlertMsg").html(message);
        $("#alert-box").attr('class', type + '-box alert').show().delay(5000).fadeOut("slow");
    }
    function closeMsg() {
        $("#alert-box").hide();
    }
    function ResetKendoFileUpload() {
        $("input[type='file']").val("");
        $(".k-upload-action").click();
    }
    GetLogo();
    function GetLogo() {
        $.ajax({
            url: "@Url.Content("~/General/GetLogo")",
            type: "POST",
            dataType: 'json',
            success: function (result) {
                
                if (result) {

                    $('#LayoutLogoPhoto').attr('src', result);



                } else {
                    $('#LayoutLogoPhoto').attr('src', "/Content/dist/img/vector/newicon/SaibBank.png");
                }
            },
            error: function (result) {

            }
        });
    }
</script>